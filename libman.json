{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.1.3",
      "files": [ "css/bootstrap.css", "css/bootstrap.min.css", "js/bootstrap.bundle.js", "js/bootstrap.bundle.min.js" ],
      "destination": "wwwroot/lib/bootstrap"
    },
    {
      "library": "jquery@3.3.1",
      "files": [ "jquery.js", "jquery.min.js" ],
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-validate@1.17.0",
      "files": [ "jquery.validate.js", "jquery.validate.min.js", "additional-methods.js", "additional-methods.min.js" ],
      "destination": "wwwroot/lib/jquery-validation/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.10",
      "files": [ "jquery.validate.unobtrusive.js", "jquery.validate.unobtrusive.min.js" ],
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    }
  ]
}